let twFilter = 'all';
const pollHasVoted = {};
const eventJoinedState = {};
const postLikedState = {};

function setTwFilter(btn, filter) {
  document.querySelectorAll('.tw-tab').forEach(t => t.classList.remove('active'));
  btn.classList.add('active');
  twFilter = filter;
  renderTwFeed();
}

function renderTwFeed() {
  const feed = document.getElementById('twFeed');
  if (!feed) return;
  feed.innerHTML = '';

  const filtered = twPosts.filter(p => {
    if (twFilter === 'all') return true;
    if (twFilter === 'events') return p.type === 'event';
    if (twFilter === 'posts')  return p.type === 'post' || p.type === 'announcement';
    if (twFilter === 'polls')  return p.type === 'poll';
    return true;
  });

  // Inject own created events into feed
  if (twFilter === 'all' || twFilter === 'events') {
    userEvents.forEach((e, idx) => {
      if (!e.isOwn) return;
      const dupe = filtered.some(p => p.type === 'event' && p.event &&
        p.event.name.toUpperCase().startsWith(e.name.toUpperCase().slice(0,5)));
      if (!dupe) {
        filtered.push({
          id: 'ue-' + idx, type: 'event',
          author: 'You', authorInitials: 'AR', authorHandle: '@alex_r',
          verified: false, accentColor: catColors[e.cat] || catColors.Default, time: 'just now',
          body: e.desc || '',
          event: { name: e.name, cat: e.cat, catColor: catColors[e.cat] || catColors.Default,
            date: e.date + ' \u00b7 ' + e.time, location: '',
            maxSpots: e.participants, joined: e.joinedCount, isJoined: true, _evtRef: e },
          likes: 0, liked: false, comments: 0, reposts: 0,
        });
      }
    });
  }

  filtered.forEach((post, i) => {
    feed.appendChild(buildTwPost(post, i));
  });
}

function buildTwPost(post, delay) {
  const ac = hostColor(post.author);
  const wrap = document.createElement('div');
  wrap.className = 'tw-post';
  wrap.style.animationDelay = (delay * 0.05) + 's';

  const verBadge = post.verified
    ? `<div class="tw-verified-badge"><svg width="8" height="8" viewBox="0 0 24 24" fill="none" stroke="#070906" stroke-width="3" stroke-linecap="round"><polyline points="20 6 9 17 4 12"/></svg></div>`
    : '';

  const typePillHtml = post.typePill
    ? `<div class="tw-type-pill" style="background:${post.typePill.color}15;color:${post.typePill.color};border-color:${post.typePill.color}35">${post.typePill.label}</div>`
    : '';

  const bodyHtml = post.body ? `<div class="tw-body">${post.body}</div>` : '';

  let innerHtml = '';
  if (post.type === 'poll') innerHtml = buildPollHtml(post);
  else if (post.type === 'event') innerHtml = buildEventCardHtml(post);
  else if (post.type === 'announcement') innerHtml = buildAnnouncementHtml(post);

  const imageHtml = post.image ? buildImageHtml(post.image) : '';

  const liked = postLikedState[post.id] !== undefined ? postLikedState[post.id] : post.liked;
  const likeCount = post.likes + (postLikedState[post.id] ? 1 : 0);

  wrap.innerHTML = `
    <div class="tw-avatar" style="background:${ac}22;color:${ac};border-color:${ac}44">${post.authorInitials}</div>
    <div class="tw-post-right">
      <div class="tw-name-row">
        <span class="tw-name">${post.author}</span>${verBadge}
        <span class="tw-handle">${post.authorHandle}</span>
        <span class="tw-dot-sep">\u00b7</span>
        <span class="tw-time">${post.time}</span>
      </div>
      ${typePillHtml}
      ${bodyHtml}
      ${imageHtml}
      ${innerHtml}
      <div class="tw-actions">
        <button class="tw-action ${liked ? 'liked' : ''}" onclick="toggleLike('${post.id}',this)">
          <svg width="14" height="14" viewBox="0 0 24 24" fill="${liked ? '#f87171' : 'none'}" stroke="${liked ? '#f87171' : 'currentColor'}" stroke-width="1.8" stroke-linecap="round"><path d="M20.84 4.61a5.5 5.5 0 00-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 00-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 000-7.78z"/></svg>
          <span class="tw-action-count">${likeCount}</span>
        </button>
        <button class="tw-action">
          <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.8" stroke-linecap="round"><path d="M21 15a2 2 0 01-2 2H7l-4 4V5a2 2 0 012-2h14a2 2 0 012 2z"/></svg>
          <span class="tw-action-count">${post.comments}</span>
        </button>
        <button class="tw-action">
          <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.8" stroke-linecap="round"><polyline points="17 1 21 5 17 9"/><path d="M3 11V9a4 4 0 014-4h14"/><polyline points="7 23 3 19 7 15"/><path d="M21 13v2a4 4 0 01-4 4H3"/></svg>
          <span class="tw-action-count">${post.reposts}</span>
        </button>
      </div>
    </div>`;
  return wrap;
}

function buildPollHtml(post) {
  const p = post.poll;
  const hasVoted = pollHasVoted[post.id] !== undefined;
  const totalVotes = p.options.reduce((s, o) => s + o.votes, 0);
  const maxVotes = Math.max(...p.options.map(o => o.votes));
  const flags = { IT: '\u{1F1EE}\u{1F1F9}', ES: '\u{1F1EA}\u{1F1F8}' };

  const optionsHtml = p.options.map((opt, i) => {
    const pct = totalVotes ? Math.round((opt.votes / totalVotes) * 100) : 0;
    const isWinner = hasVoted && opt.votes === maxVotes;
    const isMyVote = hasVoted && pollHasVoted[post.id] === i;
    const barBg = isWinner ? 'rgba(200,241,53,0.18)' : 'rgba(200,241,53,0.07)';
    const checkmark = isMyVote ? `<svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="#c8f135" stroke-width="2.5" stroke-linecap="round"><polyline points="20 6 9 17 4 12"/></svg>` : '';
    const flag = opt.flag && flags[opt.flag] ? `<span style="font-size:13px">${flags[opt.flag]}</span>` : '';
    return `
      <div class="tw-poll-option ${hasVoted ? 'voted' : ''} ${isWinner ? 'winner' : ''}"
           style="${isWinner ? 'border-color:rgba(200,241,53,0.5)' : ''}"
           onclick="${hasVoted ? '' : "castVote('" + post.id + "'," + i + ",this)"}">
        <div class="tw-poll-bar" style="width:${hasVoted ? pct : 0}%;background:${barBg}"></div>
        <div class="tw-poll-option-inner">
          <span class="tw-poll-label">${checkmark}${flag}${opt.label}</span>
          ${hasVoted ? `<span class="tw-poll-pct" style="color:${isWinner ? 'var(--lime)' : 'var(--muted)'}">${pct}%</span>` : ''}
        </div>
      </div>`;
  }).join('');

  return `
    <div class="tw-poll">
      <div class="tw-poll-header">
        <div class="tw-poll-title">${p.title}</div>
        <div class="tw-poll-sub">${p.sub}</div>
      </div>
      <div class="tw-poll-options">${optionsHtml}</div>
      <div class="tw-poll-footer">
        <span class="tw-poll-total">${totalVotes.toLocaleString()} votes \u00b7 ends in ${p.endsIn}</span>
        <span class="tw-poll-cost" id="poll-cost-${post.id}">
          <svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"/></svg>
          ${hasVoted ? 'Voted' : p.cost + ' pts to vote'}
        </span>
      </div>
    </div>`;
}

function buildEventCardHtml(post) {
  const e = post.event;
  const pct = Math.min(100, Math.round((e.joined / e.maxSpots) * 100));
  const spotsLeft = e.maxSpots - e.joined;
  const isJoined = eventJoinedState[post.id] || e.isJoined;
  const isFull = spotsLeft <= 0;
  const btnClass = isJoined ? 'joined' : (isFull ? 'full' : 'join');
  const btnLabel = isJoined ? '\u2713 Joined' : (isFull ? 'Full' : 'Join \u2192');
  const disabled = (isJoined || isFull) ? 'disabled' : '';

  return `
    <div class="tw-event-card">
      <div class="tw-event-card-top">
        <div style="display:flex;align-items:center;justify-content:space-between;margin-bottom:6px">
          <div class="tw-event-card-name">${e.name}</div>
          <div style="font-size:10px;font-weight:600;letter-spacing:0.8px;text-transform:uppercase;padding:2px 8px;border-radius:4px;background:${e.catColor}18;color:${e.catColor};border:1px solid ${e.catColor}35">${e.cat}</div>
        </div>
        <div class="tw-event-card-meta">
          <span><svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><rect x="3" y="4" width="18" height="18" rx="2"/><path d="M16 2v4M8 2v4M3 10h18"/></svg>${e.date}</span>
          ${e.location ? `<span><svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><circle cx="12" cy="10" r="3"/><path d="M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7z"/></svg>${e.location}</span>` : ''}
        </div>
      </div>
      <div class="tw-event-bar-wrap">
        <div style="display:flex;justify-content:space-between;font-size:10px;color:var(--muted);margin-top:2px">
          <span>${e.joined} / ${e.maxSpots} joined</span>
          <span style="color:${e.catColor};font-weight:500">${spotsLeft} left</span>
        </div>
        <div class="tw-event-bar"><div class="tw-event-bar-fill" style="width:${pct}%;background:${e.catColor}"></div></div>
      </div>
      <div class="tw-event-card-bottom">
        <div class="tw-event-spots">${isJoined ? '<strong style="color:var(--lime)">\u2713 You\'re in!</strong>' : spotsLeft + ' spot' + (spotsLeft !== 1 ? 's' : '') + ' remaining'}</div>
        <button class="tw-join-btn ${btnClass}" ${disabled} onclick="joinTwEvent('${post.id}',this)">${btnLabel}</button>
      </div>
    </div>`;
}

function buildAnnouncementHtml(post) {
  if (!post.announcement) return '';
  const a = post.announcement;
  return `
    <div class="tw-announcement">
      <div class="tw-announcement-label">${a.label}</div>
      <div class="tw-announcement-text">${a.text}</div>
    </div>`;
}

function buildImageHtml(img) {
  let visual = '';
  if (img.visual === 'clay_court') {
    visual = `<div style="background:linear-gradient(135deg,#3d1a08,#7a320f);width:100%;height:100%;position:relative;overflow:hidden;display:flex;align-items:center;justify-content:center">
      <svg style="position:absolute;inset:0;width:100%;height:100%;opacity:0.09" viewBox="0 0 320 160" preserveAspectRatio="xMidYMid slice">
        <rect x="20" y="10" width="280" height="140" fill="none" stroke="#f5c842" stroke-width="2"/>
        <line x1="160" y1="10" x2="160" y2="150" stroke="#f5c842" stroke-width="1.5"/>
        <line x1="20" y1="80" x2="300" y2="80" stroke="#f5c842" stroke-width="1.5"/>
        <rect x="20" y="45" width="280" height="70" fill="none" stroke="#f5c842" stroke-width="1"/>
        <line x1="160" y1="45" x2="160" y2="115" stroke="#f5c842" stroke-width="3"/>
      </svg>
      <svg width="44" height="44" viewBox="0 0 24 24" fill="none" stroke="#f5c842" stroke-width="1.2" stroke-linecap="round" opacity="0.45">
        <circle cx="12" cy="12" r="10"/><path d="M12 2C6 7 6 17 12 22"/><path d="M12 2c6 5 6 15 0 20"/><line x1="2" y1="12" x2="22" y2="12"/>
      </svg>
    </div>`;
  } else if (img.visual === 'mountains') {
    visual = `<div style="background:linear-gradient(180deg,#061020,#0e2a40);width:100%;height:100%;position:relative;overflow:hidden">
      <svg style="position:absolute;bottom:0;left:0;right:0" width="100%" height="100%" viewBox="0 0 320 160" preserveAspectRatio="xMidYMid slice">
        <polygon points="0,160 55,70 95,100 145,30 195,75 240,38 290,60 320,48 320,160" fill="#b0bec5" opacity="0.25"/>
        <polygon points="0,160 35,90 70,110 125,50 175,85 225,52 275,72 320,58 320,160" fill="#d0dde5" opacity="0.45"/>
        <polygon points="0,160 20,120 65,80 100,100 148,45 195,78 238,50 282,68 320,55 320,160" fill="#e8eef2" opacity="0.65"/>
      </svg>
    </div>`;
  }
  return `<div class="tw-img-card"><div class="tw-img-card-visual">${visual}</div><div class="tw-img-caption">${img.caption}</div></div>`;
}

function castVote(postId, optIdx, el) {
  const post = twPosts.find(p => p.id === postId);
  if (!post || !post.poll || pollHasVoted[postId] !== undefined) return;
  if (pointBalance < post.poll.cost) {
    const footer = el.closest('.tw-poll').querySelector('#poll-cost-' + postId);
    if (footer) { const orig = footer.innerHTML; footer.style.color = '#f87171'; footer.textContent = 'Not enough points!'; setTimeout(() => { footer.innerHTML = orig; footer.style.color = ''; }, 2000); }
    return;
  }
  pointBalance -= post.poll.cost;
  animateBalance('pointBalance', pointBalance.toLocaleString());
  post.poll.options[optIdx].votes++;
  pollHasVoted[postId] = optIdx;
  renderTwFeed();
}

function joinTwEvent(postId, btn) {
  const post = twPosts.find(p => p.id === postId);
  if (!post || !post.event || eventJoinedState[postId]) return;
  const e = post.event;
  if (e.maxSpots - e.joined <= 0) return;
  btn.textContent = 'Joining\u2026';
  btn.disabled = true;
  setTimeout(() => {
    eventJoinedState[postId] = true;
    e.joined++;
    e.isJoined = true;
    if (typeof e.idx === 'number') {
      const feat = featuredEvents[e.idx];
      if (feat && !featSignedUp[e.idx]) {
        featSignedUp[e.idx] = true;
        feat.filled++;
        userEvents.push({ name: feat.title, cat: feat.cat, date: feat.date, time: feat.time,
          participants: feat.maxSpots, desc: feat.desc.split('\n')[0],
          joinedCount: feat.filled, isOwn: false, host: feat.host, hostInitials: feat.hostInitials, joined: true });
      }
    }
    pointBalance += 5;
    animateBalance('pointBalance', pointBalance.toLocaleString());
    renderTwFeed();
    const calPage = document.getElementById('page-calendar');
    if (calPage && calPage.classList.contains('active')) { renderCalendar(); renderEventsList(); }
    showNotif('success', e.name + ' added to your Calendar');
  }, 600);
}

function toggleLike(postId, btn) {
  postLikedState[postId] = !(postLikedState[postId] || false);
  renderTwFeed();
}

function setFilter(btn, cat) {}
function renderSocialFeed() { renderTwFeed(); }

function joinEvent(idx, btn) {
  const evt = userEvents[idx];
  if (!evt || evt.isOwn || evt.joined) return;
  if (evt.joinedCount >= evt.participants) return;
  evt.joined = true;
  evt.joinedCount = (evt.joinedCount || 0) + 1;
  btn.textContent = 'Joining\u2026';
  btn.disabled = true;
  btn.style.opacity = '0.7';
  btn.textContent = 'Joining…';
  btn.disabled = true;
  btn.style.opacity = '0.7';

  setTimeout(() => {
    renderSocialFeed();
    // Refresh calendar dots + list if calendar page is visible
    const calPage = document.getElementById('page-calendar');
    if (calPage && calPage.classList.contains('active')) {
      renderCalendar();
      renderEventsList();
    }
    pointBalance += 5;
    animateBalance('pointBalance', pointBalance.toLocaleString());
  }, 700);
}

// ─────────────────────────────────
// CALENDAR SUB-TABS
// ─────────────────────────────────
function switchCalTab(tab) {
  document.getElementById('calTabView').classList.toggle('active', tab === 'view');
  document.getElementById('calTabCreate').classList.toggle('active', tab === 'create');
  document.getElementById('calPaneView').classList.toggle('active', tab === 'view');
  document.getElementById('calPaneCreate').classList.toggle('active', tab === 'create');
}

// ─────────────────────────────────
// CALENDAR RENDERING
// ─────────────────────────────────
const MONTHS = ['January','February','March','April','May','June','July','August','September','October','November','December'];
let calYear = 2026, calMonth = 1; // 0-indexed, Feb = 1

function renderCalendar() {
  const grid = document.getElementById('calGrid');
  grid.innerHTML = '';
  document.getElementById('calMonthTitle').textContent = `${MONTHS[calMonth].toUpperCase()} ${calYear}`;
  const firstDay = new Date(calYear, calMonth, 1).getDay();
  const daysInMonth = new Date(calYear, calMonth+1, 0).getDate();
  const daysInPrev  = new Date(calYear, calMonth, 0).getDate();
  const today = new Date();

  // prev month tail
  for (let i = firstDay - 1; i >= 0; i--) {
    const cell = document.createElement('div');
    cell.className = 'cal-cell other-month';
    cell.textContent = daysInPrev - i;
    grid.appendChild(cell);
  }
  // current month
  for (let d = 1; d <= daysInMonth; d++) {
    const cell = document.createElement('div');
    let cls = 'cal-cell';
    if (d === today.getDate() && calMonth === today.getMonth() && calYear === today.getFullYear()) cls += ' today';
    // mark days with own or joined events
    if (userEvents.some(e => {
      if (!e.isOwn && !e.joined) return false;
      const ed = new Date(e.date);
      return ed.getDate()===d && ed.getMonth()===calMonth && ed.getFullYear()===calYear;
    })) cls += ' has-event';
    cell.className = cls;
    cell.textContent = d;
    cell.onclick = () => selectDay(d, cell);
    grid.appendChild(cell);
  }
  // next month fill
  const total = firstDay + daysInMonth;
  const remaining = total % 7 === 0 ? 0 : 7 - (total % 7);
  for (let i = 1; i <= remaining; i++) {
    const cell = document.createElement('div');
    cell.className = 'cal-cell other-month';
    cell.textContent = i;
    grid.appendChild(cell);
  }
}

function selectDay(d, cell) {
  document.querySelectorAll('.cal-cell').forEach(c => c.classList.remove('selected'));
  cell.classList.add('selected');
  renderEventsList(d);
}


