const featSignedUp = [false, false, false];
let activeSheetIdx = -1;

let currentSlide = 0;
let autoSlideTimer;

function goToSlide(idx) {
  currentSlide = idx;
  const track  = document.getElementById('featuredTrack');
  const cards  = track.querySelectorAll('.featured-card');
  if (!cards.length) return;
  const card   = cards[0];
  const gap    = 14;
  const padL   = 20;
  const scrollX = idx * (card.offsetWidth + gap);
  track.style.transition = 'transform 0.45s cubic-bezier(0.4,0,0.2,1)';
  track.style.transform  = `translateX(${-(scrollX)}px)`;
  document.querySelectorAll('.feat-dot').forEach((d, i) => d.classList.toggle('active', i === idx));
  resetAutoSlide();
}

function resetAutoSlide() {
  clearInterval(autoSlideTimer);
  autoSlideTimer = setInterval(() => {
    const total = document.querySelectorAll('.featured-card').length;
    goToSlide((currentSlide + 1) % total);
  }, 3800);
}

// ─── Open detail sheet ───
function openDetailSheet(idx) {
  activeSheetIdx = idx;
  const evt = featuredEvents[idx];
  const pct = Math.round((evt.filled / evt.maxSpots) * 100);
  const signed = featSignedUp[idx];

  // Hero
  const heroBg = document.getElementById('sheetHeroBg');
  heroBg.style.background = evt.bgColor;
  heroBg.innerHTML = `<div class="fc-shapes">${evt.shapes}</div>`;

  const tag = document.getElementById('sheetTag');
  tag.textContent = evt.tag;
  tag.style.cssText = `background:${evt.tagColor}18;color:${evt.tagColor};border-color:${evt.tagColor}40`;

  document.getElementById('sheetTitle').textContent = evt.title;
  document.getElementById('sheetTagline').textContent = evt.tagline;

  // Info chips
  const d = new Date(evt.date + 'T00:00:00');
  const dateStr = d.toLocaleDateString('en-US', { weekday:'long', month:'long', day:'numeric' });
  document.getElementById('sheetInfoRow').innerHTML = `
    <div class="sheet-info-chip"><svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><rect x="3" y="4" width="18" height="18" rx="2"/><path d="M16 2v4M8 2v4M3 10h18"/></svg><div><strong>${dateStr}</strong></div></div>
    <div class="sheet-info-chip"><svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><circle cx="12" cy="12" r="9"/><path d="M12 7v5l3 3"/></svg><div><strong>${evt.time}</strong></div></div>
    <div class="sheet-info-chip"><svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><circle cx="12" cy="10" r="3"/><path d="M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7z"/></svg><div><strong>${evt.location}</strong></div></div>
    <div class="sheet-info-chip"><svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><path d="M12 2v4M12 18v4M4.93 4.93l2.83 2.83M16.24 16.24l2.83 2.83M2 12h4M18 12h4M4.93 19.07l2.83-2.83M16.24 7.76l2.83-2.83"/></svg><div><strong>${evt.duration}</strong></div></div>
  `;

  // Desc (handle newlines)
  document.getElementById('sheetDesc').innerHTML = '<p>' + evt.desc.split('\\n\\n').join('</p><p style="margin-top:12px">') + '</p>';

  // Includes
  const inc = document.getElementById('sheetIncludes');
  inc.innerHTML = evt.includes.map(item =>
    `<div class="sheet-include-item"><div class="sheet-include-dot" style="background:${evt.accentColor}"></div>${item}</div>`
  ).join('');

  // Host
  const av = document.getElementById('sheetHostAvatar');
  av.textContent = evt.hostInitials;
  av.style.background = evt.hostColor + '20';
  av.style.color = evt.hostColor;
  av.style.border = `2px solid ${evt.hostColor}40`;
  document.getElementById('sheetHostName').textContent = evt.host;
  document.getElementById('sheetHostRole').textContent = evt.hostRole;

  // Participants bar
  document.getElementById('sheetPaxCount').textContent = `${evt.filled} / ${evt.maxSpots}`;
  document.getElementById('sheetPaxCount').style.color = evt.accentColor;
  const barFill = document.getElementById('sheetBarFill');
  barFill.style.background = evt.accentColor;
  setTimeout(() => barFill.style.width = pct + '%', 80);

  // CTA
  const cta = document.getElementById('sheetCta');
  if (signed) {
    cta.textContent = '✓ Already Signed Up';
    cta.style.background = 'var(--muted)';
    cta.style.color = 'var(--dark)';
    cta.classList.add('done');
  } else {
    cta.textContent = 'Sign Up for This Event →';
    cta.style.background = evt.ctaBg;
    cta.style.color = evt.ctaColor;
    cta.classList.remove('done');
  }
  cta.style.boxShadow = signed ? 'none' : `0 8px 30px ${evt.accentColor}35`;

  // Open sheet
  document.getElementById('detailBackdrop').classList.add('open');
  document.body.style.overflow = 'hidden';
  // Reset bar for animation
  barFill.style.width = '0%';
}

function closeDetailSheet(e) {
  if (e && e.target !== document.getElementById('detailBackdrop')) return;
  document.getElementById('detailBackdrop').classList.remove('open');
  document.body.style.overflow = '';
  document.getElementById('sheetBarFill').style.width = '0%';
}

function featSignupFromSheet() {
  if (activeSheetIdx < 0) return;
  const idx = activeSheetIdx;
  if (featSignedUp[idx]) return;

  const evt = featuredEvents[idx];

  // Check for time conflict
  const conflict = userEvents.find(e => {
    if (!e.isOwn && !e.joined) return false;
    if (e.date !== evt.date) return false;
    // simple: same date same hour
    const eH = parseInt(e.time.split(':')[0]);
    const fH = parseInt(evt.time.split(':')[0]);
    return Math.abs(eH - fH) < 2;
  });

  if (conflict) {
    closeDetailSheet();
    showNotif('conflict', `You already have "${conflict.name}" at ${conflict.time} on this day.`);
    return;
  }

  // Mark signed up
  featSignedUp[idx] = true;
  evt.filled = Math.min(evt.maxSpots, evt.filled + 1);

  // Add to userEvents for calendar
  userEvents.push({
    name: evt.title,
    cat: evt.cat,
    date: evt.date,
    time: evt.time,
    participants: evt.maxSpots,
    desc: evt.desc.split('\\n')[0],
    joinedCount: evt.filled,
    isOwn: false,
    host: evt.host,
    hostInitials: evt.hostInitials,
    joined: true,
  });

  // Update card button + spots on the banner card
  const btn   = document.getElementById('fc' + idx + '-btn');
  const spots = document.getElementById('fc' + idx + '-spots');
  if (btn) { btn.textContent = '✓ Signed Up'; btn.style.opacity = '0.7'; btn.style.cursor = 'default'; btn.classList.add('signed'); }
  if (spots) spots.textContent = `${evt.filled} / ${evt.maxSpots} spots filled`;

  closeDetailSheet();

  const d = new Date(evt.date + 'T00:00:00');
  const dateStr = d.toLocaleDateString('en-US', { weekday:'short', month:'short', day:'numeric' });
  showNotif('success', `${evt.title} · ${dateStr} · ${evt.time} added to Calendar`);

  // Update calendar/social if visible
  const calPage = document.getElementById('page-calendar');
  if (calPage && calPage.classList.contains('active')) { renderCalendar(); renderEventsList(); }
  const socialPage = document.getElementById('page-social');
  if (socialPage && socialPage.classList.contains('active')) renderSocialFeed();
}

function featSignup(idx) {
  // Quick-sign from banner card (opens sheet instead)
  openDetailSheet(idx);
}

// ─── Notifications ───
let notifTimers = {};
function showNotif(type, sub) {
  const id = type === 'success' ? 'notifSuccess' : 'notifConflict';
  const subId = type === 'success' ? 'notifSuccessSub' : 'notifConflictSub';
  document.getElementById(subId).textContent = sub;
  const el = document.getElementById(id);
  el.classList.add('show');
  clearTimeout(notifTimers[id]);
  notifTimers[id] = setTimeout(() => el.classList.remove('show'), 5000);
}
function dismissNotif(id) {
  clearTimeout(notifTimers[id]);
  document.getElementById(id).classList.remove('show');
}

// Kick off auto-slide when app launches
function initFeaturedBanner() {
  resetAutoSlide();
  // Touch swipe support
  const track = document.getElementById('featuredTrack');
  if (!track) return;
  let startX = 0;
  track.addEventListener('touchstart', e => { startX = e.touches[0].clientX; }, {passive:true});
  track.addEventListener('touchend', e => {
    const dx = e.changedTouches[0].clientX - startX;
    const total = track.querySelectorAll('.featured-card').length;
    if (dx < -40) goToSlide(Math.min(currentSlide + 1, total - 1));
    if (dx >  40) goToSlide(Math.max(currentSlide - 1, 0));
  }, {passive:true});
}

function changeMonth(dir) {
  calMonth += dir;
  if (calMonth > 11) { calMonth = 0; calYear++; }
  if (calMonth < 0)  { calMonth = 11; calYear--; }
  renderCalendar(); renderEventsList();
}

