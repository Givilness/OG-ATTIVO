function renderEventsList(filterDay = null) {
  const list = document.getElementById('eventsList');
  const title = document.getElementById('eventsListTitle');
  list.innerHTML = '';

  // Show own events AND events the user has joined
  const filtered = userEvents.filter(e => {
    if (!e.isOwn && !e.joined) return false; // only own + joined
    const ed = new Date(e.date);
    if (ed.getMonth() !== calMonth || ed.getFullYear() !== calYear) return false;
    if (filterDay !== null && ed.getDate() !== filterDay) return false;
    return true;
  });

  title.textContent = filterDay
    ? `Events on ${MONTHS[calMonth]} ${filterDay}`
    : `Events in ${MONTHS[calMonth]} ${calYear}`;

  if (filtered.length === 0) {
    list.innerHTML = `<div style="font-size:13px;color:var(--muted);padding:8px 0;font-weight:300;">No events ${filterDay ? 'on this day' : 'this month'}. <span style="color:var(--lime);cursor:pointer" onclick="switchCalTab('create')">Create one →</span></div>`;
    return;
  }

  filtered.forEach((e, delay) => {
    const color = catColors[e.cat] || catColors.Default;
    const d = new Date(e.date);
    const dateStr = d.toLocaleDateString('en-US', { weekday:'short', month:'short', day:'numeric' });
    const div = document.createElement('div');
    div.className = 'event-item';
    div.style.animationDelay = delay * 0.05 + 's';
    const roleLabel = e.isOwn ? 'Organiser' : 'Joined';
    const roleBg    = e.isOwn ? 'background:var(--lime-faint);color:var(--lime);border-color:rgba(200,241,53,0.2)' : 'background:rgba(91,141,238,0.1);color:#5b9bd5;border-color:rgba(91,141,238,0.25)';
    div.innerHTML = `
      <div class="event-item-dot" style="background:${color};box-shadow:0 0 6px ${color}50"></div>
      <div class="event-item-body">
        <div class="event-item-name">${e.name}</div>
        <div class="event-item-meta">
          <span>${dateStr} · ${e.time}</span>
          <span>${e.participants} spots · ${e.joinedCount || 1} joined</span>
        </div>
      </div>
      <div style="display:flex;flex-direction:column;align-items:flex-end;gap:5px;flex-shrink:0">
        <div class="event-item-cat">${e.cat}</div>
        <div style="font-size:10px;padding:2px 8px;border-radius:100px;border:1px solid;${roleBg};letter-spacing:0.5px;white-space:nowrap">${roleLabel}</div>
      </div>
    `;
    list.appendChild(div);
  });
}

// ─────────────────────────────────
// CREATE EVENT FORM
// ─────────────────────────────────
let selectedCat = null;
let participantCount = 4;

// Set today as default date
(function setDefaultDate() {
  const today = new Date();
  const yyyy = today.getFullYear();
  const mm   = String(today.getMonth()+1).padStart(2,'0');
  const dd   = String(today.getDate()).padStart(2,'0');
  const el   = document.getElementById('evtDate');
  if (el) { el.value = `${yyyy}-${mm}-${dd}`; el.min = `${yyyy}-${mm}-${dd}`; }
})();

function selectCat(btn) {
  document.querySelectorAll('.cat-chip').forEach(c => c.classList.remove('selected'));
  btn.classList.add('selected');
  selectedCat = btn.dataset.cat;
}

function adjustParticipants(delta) {
  participantCount = Math.max(1, Math.min(50, participantCount + delta));
  document.getElementById('pCount').textContent = participantCount;
}

function submitEvent() {
  const name = document.getElementById('evtName').value.trim();
  const desc = document.getElementById('evtDesc').value.trim();
  const date = document.getElementById('evtDate').value;
  const time = document.getElementById('evtTime').value;

  if (!name)         { flashInput('evtName', 'Event name is required'); return; }
  if (!selectedCat)  { flashCats(); return; }
  if (!date)         { flashInput('evtDate', 'Please pick a date'); return; }

  // Build event object
  const evt = { name, desc, cat: selectedCat, date, time, participants: participantCount, joinedCount: 1, isOwn: true, host: 'Alex R.', hostInitials: 'AR', joined: false };
  userEvents.push(evt);

  // Fill modal
  const d = new Date(date);
  const dateStr = d.toLocaleDateString('en-US', { weekday:'long', month:'long', day:'numeric', year:'numeric' });
  document.getElementById('mecName').textContent = name;
  document.getElementById('mecCat').textContent  = selectedCat;
  document.getElementById('mecDate').textContent = `${dateStr} · ${time}`;
  document.getElementById('mecParticipants').textContent = `${participantCount} participant${participantCount!==1?'s':''} max`;

  // Refresh social if it's currently visible
  const socialPage = document.getElementById('page-social');
  if (socialPage && socialPage.classList.contains('active')) renderSocialFeed();
  // Show modal
  document.getElementById('confirmModal').classList.add('show');
}

function flashInput(id, msg) {
  const el = document.getElementById(id);
  el.style.borderColor = '#f87171';
  el.focus();
  setTimeout(() => el.style.borderColor = '', 2000);
}
function flashCats() {
  const el = document.getElementById('catScroll');
  el.style.outline = '2px solid #f87171'; el.style.borderRadius = '8px';
  setTimeout(() => el.style.outline = '', 2000);
}

function closeModal() {
  document.getElementById('confirmModal').classList.remove('show');
  // Reset form
  document.getElementById('evtName').value = '';
  document.getElementById('evtDesc').value = '';
  document.getElementById('pCount').textContent = '4';
  participantCount = 4; selectedCat = null;
  document.querySelectorAll('.cat-chip').forEach(c => c.classList.remove('selected'));
  // Navigate to calendar view
  document.querySelectorAll('.bnav-item').forEach(b => b.classList.remove('active'));
  document.querySelector('[data-page="calendar"]').classList.add('active');
  document.querySelectorAll('.page').forEach(p => p.classList.remove('active'));
  document.getElementById('page-calendar').classList.add('active');
  switchCalTab('view');
  // Sync calendar month to event month
  const evtDate = new Date(document.getElementById('evtDate').value || new Date());
  calMonth = evtDate.getMonth(); calYear = evtDate.getFullYear();
  renderCalendar(); renderEventsList();
}

// Close modal on backdrop click
document.getElementById('confirmModal').addEventListener('click', function(e) {
  if (e.target === this) closeModal();
});

// ─────────────────────────────────
// COMPOSE SHEET
// ─────────────────────────────────
let cmpPaxVal  = 4;
let cmpCostVal = 50;
let cmpType    = 'post';

function openCompose() {
  // reset all panes
  resetCompose();
  document.getElementById('composeBackdrop').classList.add('open');
  document.body.style.overflow = 'hidden';
}

function closeCompose(e) {
  if (e && e.target !== document.getElementById('composeBackdrop')) return;
  _closeCompose();
}
function _closeCompose() {
  document.getElementById('composeBackdrop').classList.remove('open');
  document.body.style.overflow = '';
}

function resetCompose() {
  // default to post type
  switchComposeType('post', document.querySelector('.compose-type-btn[data-type="post"]'));
  // clear fields
  ['cmpPostBody','cmpEvtName','cmpEvtDesc','cmpPollQuestion','cmpPollContext'].forEach(id => {
    const el = document.getElementById(id);
    if (el) el.value = '';
  });
  document.querySelectorAll('#cmpPollOptions .cmp-input').forEach(i => i.value = '');
  updateCharCount('cmpPostBody','cmpPostChars',280);
  updateCharCount('cmpEvtDesc','cmpEvtChars',200);
  // reset pax
  cmpPaxVal = 4;
  document.getElementById('cmpPaxCount').textContent = '4';
  // reset cost
  cmpCostVal = 50;
  document.getElementById('cmpPollCostVal').textContent = '50';
  // reset cats / dur chips
  document.querySelectorAll('.cmp-cat-chip').forEach(c => c.classList.remove('active'));
  document.querySelector('#cmpPostTypes .cmp-cat-chip').classList.add('active');
  document.querySelectorAll('.cmp-dur-chip').forEach(c => c.classList.remove('active'));
  document.querySelectorAll('#cmpEvtDuration .cmp-dur-chip')[1].classList.add('active');
  document.querySelectorAll('#cmpPollDuration .cmp-dur-chip')[1].classList.add('active');
  // reset date to today
  const today = new Date();
  const mm = String(today.getMonth()+1).padStart(2,'0');
  const dd = String(today.getDate()).padStart(2,'0');
  const dateEl = document.getElementById('cmpEvtDate');
  if (dateEl) dateEl.value = today.getFullYear()+'-'+mm+'-'+dd;
}

function switchComposeType(type, btn) {
  cmpType = type;
  document.querySelectorAll('.compose-type-btn').forEach(b => b.classList.remove('active'));
  btn.classList.add('active');
  document.querySelectorAll('.compose-pane').forEach(p => p.classList.remove('active'));
  const paneMap = { post:'cmpPanePost', event:'cmpPaneEvent', poll:'cmpPanePoll' };
  document.getElementById(paneMap[type]).classList.add('active');
}

function selectCmpCat(btn, groupId) {
  document.querySelectorAll('#'+groupId+' .cmp-cat-chip').forEach(c => c.classList.remove('active'));
  btn.classList.add('active');
}
function selectCmpDur(btn) {
  const group = btn.closest('.cmp-duration-row');
  group.querySelectorAll('.cmp-dur-chip').forEach(c => c.classList.remove('active'));
  btn.classList.add('active');
}
function adjustCmpPax(delta) {
  cmpPaxVal = Math.max(2, Math.min(100, cmpPaxVal + delta));
  document.getElementById('cmpPaxCount').textContent = cmpPaxVal;
}
function adjustPollCost(delta) {
  cmpCostVal = Math.max(0, Math.min(500, cmpCostVal + delta));
  document.getElementById('cmpPollCostVal').textContent = cmpCostVal;
}
function updateCharCount(textareaId, counterId, max) {
  const el = document.getElementById(textareaId);
  const counter = document.getElementById(counterId);
  if (!el || !counter) return;
  const left = max - el.value.length;
  counter.textContent = left + ' left';
  counter.className = 'cmp-char-count' + (left < 20 ? ' warn' : '') + (left < 0 ? ' over' : '');
}

// ── Submit: Post ──
function submitComposePost() {
  const body = document.getElementById('cmpPostBody').value.trim();
  if (!body) { flashInput('cmpPostBody'); return; }

  const catBtn  = document.querySelector('#cmpPostCats .cmp-cat-chip.active');
  const typeBtn = document.querySelector('#cmpPostTypes .cmp-cat-chip.active');
  const cat   = catBtn  ? catBtn.dataset.cat : null;
  const ptype = typeBtn ? typeBtn.dataset.val : 'Activity';
  const pcolor = { Achievement:'#c8f135', Activity:'#5b9bd5', 'New Record':'#f97316',
                   Community:'#a78bfa', Question:'#22d3ee' }[ptype] || '#c8f135';

  const newPost = {
    id: 'user-post-' + Date.now(), type: 'post',
    author: 'Alex R.', authorInitials: 'AR', authorHandle: '@alex_r',
    verified: false, accentColor: pcolor, time: 'just now',
    typePill: { label: ptype, color: pcolor },
    body: body,
    likes: 0, liked: false, comments: 0, reposts: 0,
  };
  twPosts.unshift(newPost);

  _closeCompose();
  renderTwFeed();
  showNotif('success', 'Your post is live in the community feed');
}

// ── Submit: Event ──
function submitComposeEvent() {
  const name = document.getElementById('cmpEvtName').value.trim();
  const desc = document.getElementById('cmpEvtDesc').value.trim();
  const date = document.getElementById('cmpEvtDate').value;
  const time = document.getElementById('cmpEvtTime').value;
  const catBtn = document.querySelector('#cmpEvtCats .cmp-cat-chip.active');

  if (!name) { flashInput('cmpEvtName'); return; }
  if (!catBtn) { flashGroup('cmpEvtCats'); return; }
  if (!date)  { flashInput('cmpEvtDate'); return; }

  const cat = catBtn.dataset.cat;
  const catColor = catColors[cat] || catColors.Default;
  const durBtn = document.querySelector('#cmpEvtDuration .cmp-dur-chip.active');
  const dur = durBtn ? durBtn.dataset.dur : '1 hr';
  const d = new Date(date + 'T00:00:00');
  const dateLabel = d.toLocaleDateString('en-US',{weekday:'short',month:'short',day:'numeric'}) + ' \u00b7 ' + time;

  // Add to userEvents
  const newEvt = {
    name, cat, date, time,
    participants: cmpPaxVal, desc: desc || name,
    joinedCount: 1, isOwn: true,
    host: 'Alex R.', hostInitials: 'AR', joined: false,
  };
  userEvents.push(newEvt);

  // Add to social feed as event post
  const newPost = {
    id: 'user-evt-' + Date.now(), type: 'event',
    author: 'Alex R.', authorInitials: 'AR', authorHandle: '@alex_r',
    verified: false, accentColor: catColor, time: 'just now',
    body: desc || 'New event — come join!',
    event: {
      name: name.toUpperCase(), cat, catColor,
      date: dateLabel, location: '',
      maxSpots: cmpPaxVal, joined: 1, isJoined: true,
    },
    likes: 0, liked: false, comments: 0, reposts: 0,
  };
  twPosts.unshift(newPost);

  _closeCompose();
  renderTwFeed();
  renderCalendar();
  renderEventsList();
  showNotif('success', name + ' created and added to your Calendar');
}

// ── Submit: Poll ──
function submitComposePoll() {
  const question = document.getElementById('cmpPollQuestion').value.trim();
  const context  = document.getElementById('cmpPollContext').value.trim();
  const inputs   = document.querySelectorAll('#cmpPollOptions .cmp-input');
  const opts = Array.from(inputs).map(i => i.value.trim()).filter(Boolean);

  if (!question) { flashInput('cmpPollQuestion'); return; }
  if (opts.length < 2) { flashGroup('cmpPollOptions'); return; }

  const durBtn = document.querySelector('#cmpPollDuration .cmp-dur-chip.active');
  const durMap = { '12h':'12 hours', '1d':'1 day', '3d':'3 days', '7d':'7 days' };
  const endsIn = durBtn ? (durMap[durBtn.dataset.dur] || durBtn.dataset.dur) : '1 day';

  const newPost = {
    id: 'user-poll-' + Date.now(), type: 'poll',
    author: 'Alex R.', authorInitials: 'AR', authorHandle: '@alex_r',
    verified: false, accentColor: '#c8f135', time: 'just now',
    body: context || '',
    poll: {
      title: question.toUpperCase(),
      sub: question,
      voted: false, votedIdx: -1,
      options: opts.map(label => ({ label, votes: 0 })),
      cost: cmpCostVal,
      endsIn,
    },
    likes: 0, liked: false, comments: 0, reposts: 0,
  };
  twPosts.unshift(newPost);

  _closeCompose();
  renderTwFeed();
  showNotif('success', 'Poll launched! Community can vote now');
}

function flashInput(id) {
  const el = document.getElementById(id);
  if (!el) return;
  el.style.transition = 'border-color 0.15s';
  el.style.borderColor = '#f87171';
  setTimeout(() => el.style.borderColor = '', 1200);
}
function flashGroup(id) {
  const el = document.getElementById(id);
  if (!el) return;
  el.style.transition = 'outline 0.15s';
  el.style.outline = '1px solid #f87171';
  el.style.borderRadius = '8px';
  setTimeout(() => { el.style.outline = ''; el.style.borderRadius = ''; }, 1200);
}


// ─────────────────────────────────
// ATTIVO CONNECT
// ─────────────────────────────────
const connectPlayers = [
  {
    initials: 'SM', name: 'Sara M.', sport: 'Tennis',
    venue: 'Parc des Eaux-Vives Court 3',
    slots: ['08:00', '09:30', '11:00'],
    connected: false,
  },
  {
    initials: 'TB', name: 'Tom B.', sport: 'Cycling',
    venue: 'Quai du Mont-Blanc Start Point',
    slots: ['07:00', '07:30', '08:00'],
    connected: false,
  },
  {
    initials: 'NK', name: 'Nina K.', sport: 'Running',
    venue: 'Parc La Grange, Main Entrance',
    slots: ['06:00', '06:30', '18:30'],
    connected: false,
  },
];

let acExpanded = false;

function toggleConnect() {
  acExpanded ? collapseConnect() : expandConnect();
}

function expandConnect() {
  acExpanded = true;
  document.getElementById('acDeck').classList.add('hidden');
  document.getElementById('acExpanded').classList.add('open');
  const btn = document.getElementById('acToggleBtn');
  const icon = document.getElementById('acToggleIcon');
  btn.classList.add('expanded');
  btn.childNodes[btn.childNodes.length - 1].textContent = ' Collapse';
  icon.innerHTML = '<path d="M18 15l-6-6-6 6"/>';
  renderConnectCards();
}

function collapseConnect() {
  acExpanded = false;
  document.getElementById('acDeck').classList.remove('hidden');
  document.getElementById('acExpanded').classList.remove('open');
  const btn = document.getElementById('acToggleBtn');
  const icon = document.getElementById('acToggleIcon');
  btn.classList.remove('expanded');
  btn.childNodes[btn.childNodes.length - 1].textContent = ' View';
  icon.innerHTML = '<path d="M6 9l6 6 6-6"/>';
}

function renderConnectCards() {
  const list = document.getElementById('acCardsList');
  list.innerHTML = '';
  connectPlayers.forEach((p, i) => {
    const card = document.createElement('div');
    card.className = 'ac-player-card';
    card.style.animationDelay = (i * 0.07) + 's';
    const slotsHtml = p.slots.map(s => `<span class="ac-slot-pill">${s}</span>`).join('');
    const btnClass = p.connected ? 'ac-connect-btn requested' : 'ac-connect-btn';
    const btnLabel = p.connected ? '\u2713 Request Sent' : 'Connect \u2192';
    card.innerHTML = `
      <div class="ac-player-top">
        <div class="ac-avatar-sm">${p.initials}</div>
        <div class="ac-player-info">
          <div class="ac-player-name">${p.name}</div>
          <span class="ac-player-sport-pill">${p.sport}</span>
        </div>
      </div>
      <div class="ac-player-divider"></div>
      <div class="ac-player-venue">
        <svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="var(--lime)" stroke-width="2" stroke-linecap="round"><circle cx="12" cy="10" r="3"/><path d="M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7z"/></svg>
        ${p.venue}
      </div>
      <div class="ac-player-slots">
        <div class="ac-slots-label">
          <svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><circle cx="12" cy="12" r="9"/><path d="M12 7v5l3 3"/></svg>
          Available Slots
        </div>
        <div class="ac-slot-pills">${slotsHtml}</div>
      </div>
      <button class="${btnClass}" onclick="sendConnect(${i},this)" ${p.connected ? 'disabled' : ''}>${btnLabel}</button>
    `;
    list.appendChild(card);
  });
}

function sendConnect(idx, btn) {
  connectPlayers[idx].connected = true;
  btn.textContent = '\u2713 Request Sent';
  btn.classList.add('requested');
  btn.disabled = true;
  showNotif('success', 'Connect request sent to ' + connectPlayers[idx].name);
}


