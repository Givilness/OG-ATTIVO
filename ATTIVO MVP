function renderProfile() {
  renderCollectibles();
  renderSportTabs();
  renderSportPanel(activeSportId);
}

let collCurrentSlide = 0;
let collSlideTimer = null;

function goToCollSlide(idx) {
  collCurrentSlide = idx;
  const track = document.getElementById('collectiblesTrack');
  const cards = track.querySelectorAll('.collectible-card');
  if (!cards.length) return;
  const cardW = cards[0].offsetWidth + 14; // 14 = gap
  track.style.transition = 'transform 0.45s cubic-bezier(0.4,0,0.2,1)';
  track.style.transform = `translateX(${-(idx * cardW)}px)`;
  document.querySelectorAll('.coll-dot').forEach((d, i) => d.classList.toggle('active', i === idx));
  resetCollSlideTimer();
}

function resetCollSlideTimer() {
  clearInterval(collSlideTimer);
  collSlideTimer = setInterval(() => {
    const total = profileCollectibles.length;
    goToCollSlide((collCurrentSlide + 1) % total);
  }, 3200);
}

function renderCollectibles() {
  const track = document.getElementById('collectiblesTrack');
  const dotsWrap = document.getElementById('collectiblesDots');
  if (!track) return;
  track.innerHTML = '';
  if (dotsWrap) dotsWrap.innerHTML = '';

  profileCollectibles.forEach((c, i) => {
    // Card
    const card = document.createElement('div');
    card.className = 'collectible-card';
    card.setAttribute('role', 'listitem');
    card.setAttribute('tabindex', '0');
    card.setAttribute('aria-label', c.name + ' \u2014 ' + c.label);
    card.innerHTML = `
      <div class="collectible-visual">
        <img src="${c.img}" alt="${c.name}">
        <div class="collectible-overlay-text">
          <span class="collectible-sport-tag" style="background:${c.accent}18;color:${c.accent};border-color:${c.accent}40">${c.sport}</span>
          <div class="collectible-name">${c.name}</div>
          <div class="collectible-sublabel">${c.label}</div>
        </div>
      </div>
      <div class="collectible-bottom">
        <span class="collectible-date">${c.date}</span>
        <button class="collectible-view-btn" style="background:${c.accent};color:#070906">View →</button>
      </div>`;
    card.onclick = () => openCollDetail(c);
    card.onkeydown = e => { if (e.key === 'Enter' || e.key === ' ') openCollDetail(c); };
    track.appendChild(card);

    // Dot
    if (dotsWrap) {
      const dot = document.createElement('div');
      dot.className = 'coll-dot' + (i === 0 ? ' active' : '');
      dot.onclick = () => goToCollSlide(i);
      dotsWrap.appendChild(dot);
    }
  });

  // Reset position
  collCurrentSlide = 0;
  track.style.transition = 'transform 0s';
  track.style.transform = 'translateX(0)';
  // Start auto-slide after render
  clearInterval(collSlideTimer);
  setTimeout(() => resetCollSlideTimer(), 400);
}

function openCollDetail(c) {
  const imgWrap = document.getElementById('collDetailImg');
  imgWrap.querySelectorAll('img').forEach(el => el.remove());
  const img = document.createElement('img');
  img.src = c.img;
  img.alt = c.name;
  imgWrap.insertBefore(img, imgWrap.querySelector('.coll-detail-img-overlay'));
  imgWrap.style.outline = '2px solid ' + c.accent + '60';
  document.getElementById('collDetailName').textContent = c.name;
  document.getElementById('collDetailMeta').innerHTML =
    `<span class="coll-meta-chip"><svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><rect x="3" y="4" width="18" height="18" rx="2"/><path d="M16 2v4M8 2v4M3 10h18"/></svg>${c.date}</span>
     <span class="coll-meta-chip"><svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="${c.accent}" stroke-width="2" stroke-linecap="round"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"/></svg>${c.sport}</span>`;
  document.getElementById('collDetailDesc').textContent = c.desc;
  document.getElementById('collOverlay').classList.add('open');
  document.body.style.overflow = 'hidden';
  // Pause auto-slide while modal is open
  clearInterval(collSlideTimer);
}

function closeCollDetail(e) {
  if (e && e.target !== document.getElementById('collOverlay')) return;
  document.getElementById('collOverlay').classList.remove('open');
  document.body.style.overflow = '';
  // Resume auto-slide
  resetCollSlideTimer();
}


function renderSportTabs() {
  const tabsWrap = document.getElementById('profSportTabs');
  if (!tabsWrap) return;
  tabsWrap.innerHTML = '';
  profileSports.forEach(s => {
    const btn = document.createElement('button');
    btn.className = 'prof-sport-tab' + (s.id === activeSportId ? ' active' : '');
    btn.setAttribute('role', 'tab');
    btn.setAttribute('aria-selected', s.id === activeSportId ? 'true' : 'false');
    btn.innerHTML = `<span class="tab-sport-name">${s.name}</span><span class="tab-sport-count">${s.count}</span>`;
    btn.onclick = () => switchSportTab(s.id);
    tabsWrap.appendChild(btn);
  });
  // + Add tab
  const addBtn = document.createElement('button');
  addBtn.className = 'prof-tab-add';
  addBtn.setAttribute('aria-label', 'Add new sport');
  addBtn.innerHTML = `<svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5" stroke-linecap="round"><line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/></svg> Add Sport`;
  addBtn.onclick = openUnlock;
  tabsWrap.appendChild(addBtn);
}

function switchSportTab(id) {
  activeSportId = id;
  renderSportTabs();
  renderSportPanel(id);
}

// ── Sport panel ──
function renderSportPanel(id) {
  const container = document.getElementById('profSportPanels');
  if (!container) return;
  const sport = profileSports.find(s => s.id === id);
  if (!sport) return;

  const snapshotHtml = sport.snapshot.map(s =>
    `<div class="prof-snap-item">
      <div class="prof-snap-val${s.accent ? ' accent' : ''}">${s.val}</div>
      <div class="prof-snap-label">${s.label}</div>
    </div>`).join('');

  const secondaryHtml = sport.secondary.map(s =>
    `<div class="prof-sec-chip">
      <div class="prof-sec-val">${s.val}</div>
      <div class="prof-sec-label">${s.label}</div>
    </div>`).join('');

  const resultsHtml = sport.results.map(r =>
    `<div class="prof-result-row">
      <div class="prof-result-date">${r.date}</div>
      <div class="prof-result-main">
        <div class="prof-result-opponent">${r.opponent}</div>
        <div class="prof-result-type">${r.type}</div>
      </div>
      <span class="prof-result-score">${r.score}</span>
      <span class="prof-result-chip ${r.win ? 'win' : 'loss'}">${r.win ? 'Win' : 'Loss'}</span>
    </div>`).join('');

  container.innerHTML = `
    <div class="prof-sport-panel" id="panel-${sport.id}">
      <div class="prof-snapshot">${snapshotHtml}</div>
      <div class="prof-secondary">${secondaryHtml}</div>
      <div class="prof-results-title">Recent Results</div>
      ${resultsHtml}
      <div class="prof-cta-row">
        <button class="prof-cta-btn primary" onclick="showNotif('success','Challenge started!')">Start Challenge \u2192</button>
        <button class="prof-cta-btn secondary" onclick="showNotif('success','Practice logged!')">Log Practice</button>
      </div>
    </div>`;
}

// ── Unlock sport modal ──
function openUnlock() {
  selectedUnlockSport = null;
  // Only offer sports not already unlocked
  const taken = new Set(profileSports.map(s => s.name.toLowerCase()));
  const opts = allSportOptions.filter(s => !taken.has(s.toLowerCase()));
  const container = document.getElementById('unlockSportOpts');
  container.innerHTML = '';
  opts.forEach(s => {
    const btn = document.createElement('button');
    btn.className = 'unlock-sport-opt';
    btn.textContent = s;
    btn.onclick = () => {
      container.querySelectorAll('.unlock-sport-opt').forEach(b => b.classList.remove('selected'));
      btn.classList.add('selected');
      selectedUnlockSport = s;
      document.getElementById('unlockConfirmBtn').disabled = false;
    };
    container.appendChild(btn);
  });
  document.getElementById('unlockConfirmBtn').disabled = true;
  document.getElementById('unlockBackdrop').classList.add('open');
  document.body.style.overflow = 'hidden';
}

function closeUnlock(e) {
  if (e && e.target !== document.getElementById('unlockBackdrop')) return;
  document.getElementById('unlockBackdrop').classList.remove('open');
  document.body.style.overflow = '';
}

function confirmUnlock() {
  if (!selectedUnlockSport) return;
  const cost = 50;
  if (pointBalance < cost) {
    closeUnlock();
    showNotif('conflict', 'Not enough points to unlock ' + selectedUnlockSport);
    return;
  }
  const btn = document.getElementById('unlockConfirmBtn');
  btn.disabled = true;
  btn.textContent = 'Processing\u2026';

  setTimeout(() => {
    pointBalance -= cost;
    animateBalance('pointBalance', pointBalance.toLocaleString());

    // Add sport tab
    const newSport = {
      id: selectedUnlockSport.toLowerCase(),
      name: selectedUnlockSport,
      count: '0 sessions',
      snapshot: [
        { val: '0', label: 'Sessions' },
        { val: '0-0', label: 'W / L', accent: true },
        { val: '\u2014', label: 'Rating' },
      ],
      secondary: [
        { val: '0', label: 'Streak' },
        { val: '0', label: 'Min / wk' },
        { val: '0', label: 'Points' },
      ],
      results: [],
    };
    profileSports.push(newSport);

    const sportName = selectedUnlockSport;
    closeUnlock();
    document.body.style.overflow = '';

    // Render & switch to new tab
    renderSportTabs();
    switchSportTab(newSport.id);

    // Scroll new tab into view
    setTimeout(() => {
      const tabs = document.querySelectorAll('.prof-sport-tab');
      if (tabs.length) tabs[tabs.length-1].scrollIntoView({ behavior:'smooth', inline:'center' });
    }, 100);

    // Tooltip
    showProfTooltip('You can now log workouts for ' + sportName + ' \u2014 start your first session!');
  }, 900);
}

function showProfTooltip(msg) {
  const t = document.getElementById('profTooltip');
  t.innerHTML = `<svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="var(--black)" stroke-width="2.5" stroke-linecap="round"><polyline points="20 6 9 17 4 12"/></svg><span>${msg}</span>`;
  t.classList.add('show');
  setTimeout(() => t.classList.remove('show'), 4000);
}


