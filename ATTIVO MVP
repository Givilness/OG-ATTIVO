function renderStore() {
  // Update points display
  const bal = document.getElementById('mktPointsBal');
  if (bal) bal.textContent = pointBalance.toLocaleString();

  renderMktCats();
  renderMktHero();
  renderMktGear();
  renderMktExperiences();
  renderMktServices();
}

function renderMktCats() {
  const wrap = document.getElementById('mktCats');
  if (!wrap) return;
  wrap.innerHTML = '';
  mktCategories.forEach(cat => {
    const btn = document.createElement('button');
    btn.className = 'mkt-cat' + (cat === mktActiveCat ? ' active' : '');
    btn.textContent = cat;
    btn.onclick = () => { mktActiveCat = cat; renderMktCats(); renderMktGear(); renderMktExperiences(); renderMktServices(); };
    wrap.appendChild(btn);
  });
}

function mktVisible(p) {
  if (mktActiveCat === 'All') return true;
  return p.category === mktActiveCat ||
    (mktActiveCat === 'Experiences' && p.section === 'experience') ||
    (mktActiveCat === 'Services' && p.section === 'service') ||
    (mktActiveCat === 'Apparel' && p.category === 'Apparel');
}

function buildMktArt(p, sz=72) {
  return `<div class="mkt-card-art">
    <div class="mkt-card-art-glow" style="background:radial-gradient(circle at 50% 40%, ${p.color}60, transparent 70%)"></div>
    ${p.artFn(sz, p.color)}
  </div>`;
}

function buildExpArt(p, sz=64) {
  return `<div class="mkt-exp-art">
    <div class="mkt-exp-art-glow" style="background:radial-gradient(circle at 50% 40%, ${p.color}60, transparent 70%)"></div>
    ${p.artFn(sz, p.color)}
  </div>`;
}

function ptsLabel(p) {
  if (p.ptsForFull > 0) return `or ${p.ptsForFull.toLocaleString()} pts`;
  if (p.ptsDiscount > 0) return `-${p.ptsDiscount} pts`;
  return '';
}

function renderMktHero() {
  const hero = mktProducts[0];
  const art = document.getElementById('mktHeroArt');
  if (art) {
    art.style.cssText = `width:100%;height:100%;display:flex;align-items:center;justify-content:center;position:relative;overflow:hidden;background:${hero.color}10`;
    art.innerHTML = `<div style="position:absolute;inset:0;background:radial-gradient(circle at 30% 50%, ${hero.color}30, transparent 70%)"></div>${hero.artFn(90, hero.color)}`;
  }
}

function renderMktGear() {
  const grid = document.getElementById('mktGearGrid');
  if (!grid) return;
  const items = mktProducts.filter(p => p.section === 'gear' && mktVisible(p));
  grid.innerHTML = '';
  if (!items.length) { grid.innerHTML = '<div style="padding:20px;color:var(--muted);font-size:13px;grid-column:span 2">No items in this category</div>'; return; }
  items.forEach(p => {
    const el = document.createElement('div');
    el.className = 'mkt-card';
    el.innerHTML = `
      <div class="mkt-card-img">
        ${buildMktArt(p)}
        <span class="mkt-card-tag" style="background:${p.color}18;color:${p.color};border:1px solid ${p.color}30">${p.tag}</span>
        <button class="mkt-card-wishlist" onclick="event.stopPropagation()">
          <svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><path d="M20.84 4.61a5.5 5.5 0 00-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 00-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 000-7.78z"/></svg>
        </button>
      </div>
      <div class="mkt-card-body">
        <div class="mkt-card-name">${p.name}</div>
        <div class="mkt-card-brand">${p.brand}</div>
        <div class="mkt-card-price-row">
          <span class="mkt-card-cash${p.currency==='USDC'?' usdc':''}">${p.currency} ${p.cash}</span>
          <span class="mkt-card-pts">${ptsLabel(p)}</span>
          <button class="mkt-card-add" onclick="event.stopPropagation();addToCart(${p.id})">
            <svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="var(--black)" stroke-width="2.5" stroke-linecap="round"><line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/></svg>
          </button>
        </div>
      </div>`;
    el.onclick = () => openMktModal(p.id);
    grid.appendChild(el);
  });
}

function renderMktExperiences() {
  const strip = document.getElementById('mktExpStrip');
  if (!strip) return;
  const items = mktProducts.filter(p => p.section === 'experience' && mktVisible(p));
  strip.innerHTML = '';
  if (!items.length) { strip.innerHTML = '<div style="padding:0 0 8px;color:var(--muted);font-size:13px">No experiences in this category</div>'; return; }
  items.forEach(p => {
    const el = document.createElement('div');
    el.className = 'mkt-exp-card';
    el.innerHTML = `
      <div class="mkt-exp-img">${buildExpArt(p)}</div>
      <div class="mkt-exp-body">
        <div class="mkt-exp-name">${p.name}</div>
        <div class="mkt-exp-sub">${p.brand}</div>
        <div class="mkt-exp-price-row">
          <span class="mkt-exp-cash${p.currency==='USDC'?' usdc':''}">${p.currency} ${p.cash}</span>
          <span class="mkt-exp-pts">${ptsLabel(p)}</span>
        </div>
      </div>`;
    el.onclick = () => openMktModal(p.id);
    strip.appendChild(el);
  });
}

function renderMktServices() {
  const grid = document.getElementById('mktServGrid');
  if (!grid) return;
  const items = mktProducts.filter(p => p.section === 'service' && mktVisible(p));
  grid.innerHTML = '';
  if (!items.length) { grid.innerHTML = '<div style="padding:20px;color:var(--muted);font-size:13px;grid-column:span 2">No services in this category</div>'; return; }
  items.forEach(p => {
    const el = document.createElement('div');
    el.className = 'mkt-card';
    el.innerHTML = `
      <div class="mkt-card-img">
        ${buildMktArt(p)}
        <span class="mkt-card-tag" style="background:${p.color}18;color:${p.color};border:1px solid ${p.color}30">${p.tag}</span>
        <button class="mkt-card-wishlist" onclick="event.stopPropagation()">
          <svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"><path d="M20.84 4.61a5.5 5.5 0 00-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 00-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 000-7.78z"/></svg>
        </button>
      </div>
      <div class="mkt-card-body">
        <div class="mkt-card-name">${p.name}</div>
        <div class="mkt-card-brand">${p.brand}</div>
        <div class="mkt-card-price-row">
          <span class="mkt-card-cash${p.currency==='USDC'?' usdc':''}">${p.currency} ${p.cash}</span>
          <span class="mkt-card-pts">${ptsLabel(p)}</span>
          <button class="mkt-card-add" onclick="event.stopPropagation();addToCart(${p.id})">
            <svg width="13" height="13" viewBox="0 0 24 24" fill="none" stroke="var(--black)" stroke-width="2.5" stroke-linecap="round"><line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/></svg>
          </button>
        </div>
      </div>`;
    el.onclick = () => openMktModal(p.id);
    grid.appendChild(el);
  });
}

function filterStore(val) {
  const q = val.toLowerCase().trim();
  document.querySelectorAll('.mkt-card, .mkt-exp-card').forEach(el => {
    const name = el.querySelector('.mkt-card-name, .mkt-exp-name');
    const match = !q || (name && name.textContent.toLowerCase().includes(q));
    el.style.display = match ? '' : 'none';
  });
}

function openMktModal(id) {
  const p = mktProducts.find(x => x.id === id);
  if (!p) return;
  mktActiveItem = p;

  const art = document.getElementById('mktModalArt');
  art.style.background = p.color + '12';
  document.getElementById('mktModalGlow').style.background =
    `radial-gradient(circle at 40% 50%, ${p.color}50, transparent 70%)`;
  document.getElementById('mktModalArtIcon').innerHTML = p.artFn(80, p.color);

  const tag = document.getElementById('mktModalTag');
  tag.textContent = p.tag;
  tag.style.cssText = `background:${p.color}18;color:${p.color};border-color:${p.color}40`;

  document.getElementById('mktModalName').textContent = p.name;
  document.getElementById('mktModalBrand').textContent = p.brand;
  document.getElementById('mktModalDesc').textContent = p.desc;

  const hl = document.getElementById('mktModalHighlights');
  hl.innerHTML = p.highlights.map(h =>
    `<div class="mkt-modal-chip">
      <svg width="11" height="11" viewBox="0 0 24 24" fill="none" stroke="${p.color}" stroke-width="2.5" stroke-linecap="round"><polyline points="20 6 9 17 4 12"/></svg>
      ${h}
    </div>`).join('');

  document.getElementById('mktModalPrice').textContent = `${p.currency} ${p.cash}`;
  document.getElementById('mktModalPrice').className = 'mkt-modal-price-main' + (p.currency==='USDC' ? ' usdc' : '');
  const ptsOff = document.getElementById('mktModalPtsOff');
  const ptsNote = document.getElementById('mktModalPtsNote');
  if (p.ptsForFull > 0) {
    ptsOff.textContent = `${p.ptsForFull.toLocaleString()} pts`;
    ptsNote.textContent = 'use points to pay in full';
  } else if (p.ptsDiscount > 0) {
    ptsOff.textContent = `-${p.ptsDiscount} pts`;
    const discounted = p.cash - Math.floor(p.ptsDiscount * 0.1);
    ptsNote.textContent = `apply ${p.ptsDiscount} pts → ${p.currency} ${discounted}`;
  } else {
    ptsOff.textContent = '';
    ptsNote.textContent = 'no points discount available';
  }

  document.getElementById('mktModalBackdrop').classList.add('open');
  document.body.style.overflow = 'hidden';
}

function closeMktModal(e) {
  if (e && e.target !== document.getElementById('mktModalBackdrop')) return;
  document.getElementById('mktModalBackdrop').classList.remove('open');
  document.body.style.overflow = '';
}

function toggleMktWishlist() {
  const btn = document.getElementById('mktModalWishBtn');
  const svg = btn.querySelector('svg');
  const liked = btn.classList.toggle('liked');
  svg.setAttribute('fill', liked ? '#f87171' : 'none');
  svg.setAttribute('stroke', liked ? '#f87171' : 'currentColor');
  showNotif('success', liked ? 'Added to wishlist' : 'Removed from wishlist');
}

function addToCart(id) {
  const p = mktProducts.find(x => x.id === id);
  if (!p) return;
  mktCartCount++;
  showNotif('success', p.name + ' added to cart');
}

function buyMktItem() {
  if (!mktActiveItem) return;
  mktCartCount++;
  closeMktModal();
  showNotif('success', mktActiveItem.name + ' added to cart — checkout coming soon!');
}

</script>

